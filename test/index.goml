<goml width="fit" height="fit">
  <import-material typeName="lambert" src="./index.sort" />
  <material id="main" type="lambert" color="#933" sun="n(1,-1.1,-1.2)" power="10" />
  <geometry type="torus" name="torus"></geometry>
  <geometry type="torus" name="torus-small" irad="0.7" orad="1.0"></geometry>
  <geometry type="torus" name="torus-big" irad="0.1" orad="1.0"></geometry>
  <renderer camera=".camera" viewport="0,0,100%,100%">
    <render-scene/>
  </renderer>
  <scene>
    <camera class="camera" near="0.01" far="100.0" aspect="1.0" fovy="45d" position="0,0,13" rotation="0,0,0" >
      <camera.components>
        <MouseCameraControl rotateX="10" moveSpeed="1" center="13"/>
      </camera.components>
    </camera>
    <mesh position="2.3,0,0" geometry="torus-big" material="#main" targetBuffer="default" rotation="z(90)">
      <mesh.components>
        <AABB color="gray"/>
      </mesh.components>
    </mesh>
    <mesh position="-2.3,0,0" geometry="torus-small" material="#main" targetBuffer="default" rotation="z(90)">
      <mesh.components>
        <AABB color="gray"/>
      </mesh.components>
    </mesh>
    <mesh position="0,0,0" geometry="torus" material="#main" targetBuffer="default">
      <mesh.components>
        <AABB color="gray"/>
      </mesh.components>
    </mesh>
  </scene>
</goml>

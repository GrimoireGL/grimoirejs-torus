{
  "name": "grimoirejs-torus",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "grimoirejs-fundamental": "beta",
    "grimoirejs-math": "^1.8.1"
  },
  "scripts": {
    "build": "browserify -v -d index.babel.js -t [ babelify ] | exorcist index.js.map -b ./ > index.js",
    "watch": "watchify -v -d index.babel.js -t [ babelify ] -o 'exorcist index.js.map > index.js'"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "exorcist": "^0.4.0",
    "grimoirejs": "beta",
    "grimoirejs-forward-shading": "beta",
    "watchify": "^3.7.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}

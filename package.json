    "exorcist": "^0.4.0",
    "grimoirejs-aabb-debug": "git://github.com/GrimoireGL/grimoirejs-aabb-debug.git#master",
    "grimoirejs-preset-basic": "^1.2.3",
    "watchify": "^3.7.0"
  }
}
